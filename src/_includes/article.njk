---
layout: 'base.njk'
---
<article class="main-article">
<div class="container container--narrow flow">
  
    
    
  
</div>
</article>

<section id="four" class="wrapper alt style1 ">
    <div class="inner article__p">
      <h1 class="major">
        {{ title }}
      </h1>
    <figure class="main-article__figure">
      <img class="article-image" src="{{image}}" alt="{{imageAlt}}">
      <figcaption>
        {{ imageAlt }}
      </figcaption>
    </figure>

    {{ content | safe }}

    <p class="p__author">By <span>{{author}}</span> ON {{date}}</p>

    <a href="/blog" class="special">SEE MORE ARTICLES</a>
    </div>
</section>
   
